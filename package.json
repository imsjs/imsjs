  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build:constants": "pnpm --filter ./packages/ims-ui-constants run build",
    "build:enums": "pnpm --filter ./packages/ims-ui-enums run build",
    "build:utils": "pnpm --filter ./packages/ims-ui-utils run build",
    "build:hooks": "pnpm --filter ./packages/ims-ui-hooks run build",
    "build:types": "pnpm --filter ./packages/ims-ui-types run build",
    "build:components": "pnpm --filter ./packages/ims-ui-components run build",
    "build:ims-ui": "pnpm --filter ./packages/ims-ui run build",
    "build": "pnpm --filter ./packages/** run build",
    "release": "tsx build/scripts/release.ts",
    "type:app": "vue-tsc -p tsconfig.app.json --noEmit --composite false",
    "type:mv": "tsx build/scripts/dts-mv.ts",
    "dev": "pnpm --filter ./playground run dev",
    "dev:designer": "pnpm --filter ./designer run dev",
    "dev:docs": "pnpm --filter docs run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
