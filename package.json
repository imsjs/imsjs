    "build": "pnpm --filter ./packages/** run build",
    "release": "tsx build/scripts/release.ts",
    "type:app": "vue-tsc -p tsconfig.app.json --noEmit --composite false",
    "type:mv": "tsx build/scripts/dts-mv.ts",
    "dev": "pnpm --filter ./playground run dev",
    "dev:designer": "pnpm --filter ./designer run dev",
    "dev:docs": "pnpm --filter docs run dev",
